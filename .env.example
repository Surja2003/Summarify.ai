# Copy this file to `.env` for local development.
# Never commit your real `.env` file.

# OpenAI (used by /api/chat and optional backend refinement)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini

# Optional: Hugging Face hosted inference API
HUGGINGFACE_API_TOKEN=
HUGGINGFACE_MODEL=HuggingFaceH4/zephyr-7b-beta

# Optional: Backend URL for the frontend to call (if you deploy FastAPI)
VITE_BACKEND_URL=

# Optional: CORS allowlist (FastAPI backend)
# Example: https://your-vercel-app.vercel.app,http://localhost:5173
CORS_ORIGINS=
